@model IEnumerable<Case.Models.DataModel.Personel>

@{
    ViewBag.Title = "Index";
}

<h2>All Personels </h2>

<p>
    @Html.ActionLink("Yeni Personel Ekle", "Create")

    
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SicilNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Soyad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IseGirisTarihi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IstenCikisTarihi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EvTelefon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CepTelefonu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cinsiyet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepartmanCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnvanCode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SicilNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Soyad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IseGirisTarihi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IstenCikisTarihi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EvTelefon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CepTelefonu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cinsiyet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DepartmanCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnvanCode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
